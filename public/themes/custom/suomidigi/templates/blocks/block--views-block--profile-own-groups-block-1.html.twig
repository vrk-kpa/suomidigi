{#
/**
 * @file
 * Theme override to display a block.
 */
#}
{%
  set classes = [
  'block',
  'profile',
  'profile--' ~ configuration.provider|clean_class,
  'profile--' ~ plugin_id|clean_class,
  'profile__my-groups'
]
%}
<div{{ attributes.addClass(classes) }}>
  {{ title_prefix }}
  {% if label %}
    <h2{{ title_attributes }}>{{ label }}</h2>
  {% endif %}
  {{ title_suffix }}
  {% block content %}
    {{ content }}
  {% endblock %}
</div>
