                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "sortablejs/sortable",
                "version": "1.8.4",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "Sortable"
                },
                "dist": {
                    "url": "https://github.com/SortableJS/Sortable/archive/1.8.4.zip",
