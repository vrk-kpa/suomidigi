                "type": "drupal-library",
                "extra": {
                    "installer-name": "ckeditor-wordcount-plugin"
                },
                "dist": {
                    "url": "https://github.com/w8tcha/ckeditor-wordcount-plugin/archive/v1.17.6.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "jonthornton/jquery-timepicker",
                "version": "1.11.15",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "jonthornton-jquery-timepicker"
                },
                "dist": {
                    "url": "https://github.com/jonthornton/jquery-timepicker/archive/1.11.15.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "dimsemenov/magnific-popup",
                "version": "1.1.0",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "magnific-popup"
                },
                "dist": {
                    "url": "https://github.com/dimsemenov/Magnific-Popup/archive/1.1.0.zip",
