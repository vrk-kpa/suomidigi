        "npm-asset/sortablejs": "1.8.4",
        "npm-asset/timepicker": "1.11.15",
        "oomphinc/composer-installers-extender": "^2.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "config": {
        "sort-packages": true,
        "discard-changes": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "oomphinc/composer-installers-extender": true
        }
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "drupal-scaffold": {
            "allowed-packages": [
                "drupal/core"
            ],
            "locations": {
                "web-root": "./public"
            },
            "file-mapping": {
                "[project-root]/.editorconfig": false,
                "[web-root]/sites/default/default.services.yml": false,
                "[web-root]/sites/default/default.settings.php": false
            }
        },
        "enable-patching": true,
        "installer-paths": {
            "public/core": [
                "type:drupal-core"
            ],
            "public/libraries/jonthornton-jquery-timepicker": ["npm-asset/timepicker"],
            "public/libraries/jquery-timepicker-addon": ["npm-asset/jquery-ui-timepicker-addon"],
            "public/libraries/jquery-ui-timepicker": ["bower-asset/jquery.ui.timepicker"],
            "public/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "public/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "public/modules/custom/{$name}": [
                "type:drupal-module-custom"
            ],
            "public/profiles/{$name}": [
                "type:drupal-profile"
            ],
            "public/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "public/themes/custom/{$name}": [
                "type:drupal-theme-custom"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ]
        },
        "installer-types": ["bower-asset", "npm-asset"],
        "patches": {
            "drupal/core": {
                "https://www.drupal.org/i/2466553": "https://www.drupal.org/files/issues/2021-10-12/2466553-95.patch",
                "https://www.drupal.org/i/3008119": "https://www.drupal.org/files/issues/2022-01-12/3008119-62.patch",
                "https://www.drupal.org/i/1088112": "https://www.drupal.org/files/issues/2021-03-21/1088112-69.patch",
                "https://www.drupal.org/i/3150294": "https://www.drupal.org/files/issues/2022-05-13/3150294-115.patch",
                "https://www.drupal.org/i/3101344": "https://www.drupal.org/files/issues/2021-11-15/drupal-path-error-when-not-visible-3101344-81-D9.patch",
                "Remove node_user_predelete() hook to prevent deletion of nodes when deleting a user account.": "patches/remove_node_user_predelete_hook.patch",
                "Fix GTM https://www.drupal.org/project/drupal/issues/3114467":"https://www.drupal.org/files/issues/2021-11-18/3114467-25.patch"
            },
            "drupal/field_group": {
                "Add title to horizontal tabs on field groups.": "patches/field_group_horizontal_tabs_title.patch"
            },
            "drupal/flag": {
                "https://www.drupal.org/i/2982147": "https://www.drupal.org/files/issues/2018-12-12/containerAttrs.7.2982147.patch",
                "Fix classes on Flag module ajax callbacks": "patches/flag_clean_classes_on_ajax_callbacks.patch",
                "https://www.drupal.org/i/5366574": "https://www.drupal.org/files/issues/2022-04-07/5366574-str_replace-null-deprecation.patch"
            },
            "drupal/group": {
                "https://www.drupal.org/i/2942657": "https://www.drupal.org/files/issues/2019-06-27/group_route_ajax_route-2942657-18.patch",
                "Move group operations links alter to more suitable spot.": "patches/group_operations_links_alter.patch",
                "Group content Path automated pattern and tokens for actual node": "https://www.drupal.org/files/issues/2021-09-29/group-content-entity-tokens-2916907-23.patch",
                "https://www.drupal.org/i/3101314": "https://www.drupal.org/files/issues/2019-12-16/reword_delete_to_remove_on_group_member_removal-3101314-2.patch",
                "needs hook changes? https://www.drupal.org/i/2797793": "https://www.drupal.org/files/issues/2021-08-24/2797793-279.patch",
                "https://www.drupal.org/i/2980034": "https://www.drupal.org/files/issues/2021-03-18/joined_user_redirection-2980034-18.patch"
            },
            "drupal/menu_block_current_language": {
                "https://www.drupal.org/i/2952846": "https://www.drupal.org/files/issues/2018-03-14/show-siblings-of-active-parent-2952846-3_0.patch"
            },
            "drupal/menu_trail_by_path": {
                "https://www.drupal.org/i/2914746": "https://www.drupal.org/files/issues/2022-04-06/2914746-27.patch"
            },
            "drupal/paragraphs": {
                "https://www.drupal.org/i/2904705": "https://www.drupal.org/files/issues/2020-11-27/paragraphs_support_asym_translations-2904705-122.patch"
            },
            "drupal/nodeorder": {
                "https://www.drupal.org/i/3072720": "https://www.drupal.org/files/issues/2022-05-19/nodeorder-3072720-116.patch"
            }
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
